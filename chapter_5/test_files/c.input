ENUM_RETURN s_squeeze(_S8 *source, const _S8 *target)
{
    R_ASSERT(source != NULL, RETURN_FAILURE);
    R_ASSERT(target != NULL, RETURN_FAILURE);

    R_FALSE_RET(strlen(target) > 0, RETURN_SUCCESS);
    
    _S8 s[MAX_CHAR] = {0};
    _S32 c;
    while((c = *target) != '\0')
    {
        s[c] = 1;
        target++;
    }
    _S8 *p = source;
    while((c = *source) != '\0')
    {
        if(s[c] == 0)
        {
            *p = *source;
            p++;
        }
        source++;
    }
    *p = '\0';

    return RETURN_SUCCESS;
}